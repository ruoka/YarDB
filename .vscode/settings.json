{
    "files.associations": {
        "*.hpp": "cpp",
        "*.cpp": "cpp",
        "*.c++": "cpp",
        "*.c++m": "cpp"
    },
    "C_Cpp.default.compilerPath": "/usr/lib/llvm-18/bin/clang++",
    "C_Cpp.default.cppStandard": "c++23",
    "C_Cpp.default.includePath": ["${workspaceFolder}/src","${workspaceFolder}/include"],"${workspaceFolder}/YarDB"],
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.errorSquiggles": "enabled",
    "terminal.integrated.gpuAcceleration": "off",
    "C_Cpp.default.configurationProvider": "ms-vscode.makefile-tools",
    "makefile.launchConfigurations": [
        {
            "cwd": "/workspaces/YarDB/bin",
            "binaryPath": "/workspaces/YarDB/bin/test_runner",
            "binaryArgs": []
        }
    ]
}
